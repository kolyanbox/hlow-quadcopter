# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
quadcopter.hex \

SECONDARY_LIST += \
quadcopter.lst \

SECONDARY_SIZE += \
quadcopter.siz \


# All Target
all: quadcopter.elf secondary-outputs

# Tool invocations
quadcopter.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: Cross ARM C++ Linker'
	arm-none-eabi-g++ -mcpu=cortex-m3 -mthumb -O0 -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -Wall  -g3 -T link.ld -Xlinker --gc-sections -L"D:/hlow-quadcopter/branches/mbed software/hlow-quadcopter" -Wl,-Map,"quadcopter.map" -o "quadcopter.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

quadcopter.hex: quadcopter.elf
	@echo 'Invoking: Cross ARM GNU Create Flash Image'
	arm-none-eabi-objcopy "quadcopter.elf" -O ihex  "quadcopter.hex"
	@echo 'Finished building: $@'
	@echo ' '

quadcopter.lst: quadcopter.elf
	@echo 'Invoking: Cross ARM GNU Create Listing'
	arm-none-eabi-objdump "quadcopter.elf" --source --all-headers --demangle --line-numbers --wide > "quadcopter.lst"
	@echo 'Finished building: $@'
	@echo ' '

quadcopter.siz: quadcopter.elf
	@echo 'Invoking: Cross ARM GNU Print Size'
	arm-none-eabi-size  --format=berkeley "quadcopter.elf"
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(SECONDARY_SIZE)$(C++_DEPS)$(OBJS)$(C_DEPS)$(ASM_DEPS)$(CC_DEPS)$(SECONDARY_FLASH)$(CPP_DEPS)$(CXX_DEPS)$(C_UPPER_DEPS)$(SECONDARY_LIST)$(S_UPPER_DEPS) quadcopter.elf
	-@echo ' '

post-build:
	-arm-none-eabi-objcopy -O binary  quadcopter.elf  quadcopter.bin
	-@echo ' '

secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents
.SECONDARY: post-build

-include ../makefile.targets
